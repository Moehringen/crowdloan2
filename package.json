{
  "name": "subql-spectator",
  "version": "0.1.0",
  "description": "Substrate slots auction data collector",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run codegen",
    "build": "tsc -b",
    "prepack": "rm -rf dist && npm build",
    "test": "jest",
    "prestart": "npm run build",
    "predebug": "npm run build",
    "start": "subql-node -f ./ --local --batch-size 1 -- --unhandled-rejections=strict",
    "debug": "node --inspect --unhandled-rejections=strict node_modules/.bin/subql-node -f ./ --local --debug --batch-size 1",
    "query": "subql-query -n subql-spectator --playground",
    "codegen": "./node_modules/.bin/subql codegen"
  },
  "homepage": "https://github.com/subquery/subql-starter",
  "repository": "github:subquery/subql-starter",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "Kerwin Weng",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polkadot/api": "^4.10.2-4",
    "@subql/cli": "^0.9.3",
    "@subql/types": "^0.7.1",
    "typescript": "^4.1.3"
  }
}
